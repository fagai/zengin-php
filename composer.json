{
    "name": "fagai/zengin-code",
    "description": "",
    "type": "library",
    "license": "MIT",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "zengin-code/source-data",
                "version": "dev-master",
                "dist": {
                    "url": "https://github.com/zengin-code/source-data/archive/master.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "php": "^7.2",
        "zengin-code/source-data": "dev-master",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "psr-4": {
            "Fagai\\ZenginCode\\": "src/"
        }
    },
    "scripts": {
        "cs": "phpcs --standard=phpcs.xml ./src",
        "cbf": "phpcbf --standard=phpcs.xml ./src"
    }
}
